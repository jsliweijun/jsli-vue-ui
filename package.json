{
    "name": "vue2-ui",
    "version": "0.1.0",
    "main": "./dist/vue2-ui.umd.min.js",
    "private": false,
    "scripts": {
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "test": "karma start",
        "lib": "vue-cli-service build --target lib --name vue2-ui ./src/packages/index.js",
        "docs:dev": "vuepress dev docs"
    },
    "dependencies": {
        "@types/core-js": "^2.5.5",
        "core-js": "^3.6.5",
        "vue": "^2.6.11"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/eslint": "^8.4.1",
        "@types/karma": "^6.3.3",
        "@types/karma-chrome-launcher": "^3.1.1",
        "@types/karma-mocha": "^1.3.1",
        "@types/karma-spec-reporter": "^0.0.3",
        "@types/karma-webpack": "^2.0.11",
        "@types/mocha": "^9.1.0",
        "@vue/cli-plugin-babel": "~4.5.15",
        "@vue/cli-plugin-eslint": "~4.5.15",
        "@vue/cli-service": "~4.5.15",
        "@vue/test-utils": "^1.3.0",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.6",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "karma": "^6.3.17",
        "karma-chrome-launcher": "^3.1.1",
        "karma-mocha": "^2.0.1",
        "karma-sourcemap-loader": "^0.3.8",
        "karma-spec-reporter": "^0.0.33",
        "karma-webpack": "^4.0.2",
        "mocha": "^9.2.2",
        "sass": "^1.49.9",
        "sass-loader": "^10.2.1",
        "vue-template-compiler": "^2.6.11",
        "vuepress": "^1.9.7"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "no-unused-vars": 0,
            "no-undef": 0
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
